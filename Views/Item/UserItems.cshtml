@{
    ViewData["Title"] = "My Items";
}

<h2>@ViewData["Title"]</h2>

<div class="item-container">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Text</th>
                <th>CreationDate</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                   
                    <td>@item.Name</td>
                    <td>@item.Text</td>
                    <td>@item.CreationDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>